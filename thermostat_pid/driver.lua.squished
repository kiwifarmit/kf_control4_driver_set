function switch_heat_pid(state)
  LOGGER:debug("switch_heat_pid: ", state)
  if state == "OPEN" then
    C4:SendToProxy(HEATING_PID_OUTPUT_BIND, "OPEN", "NOTIFY")
    C4:SendToProxy(HEATING_INPUT_BIND, "OPENED", "NOTIFY")
