  GlobalData["HEATING_PID_OUTPUT_BIND"] = TMP_STORED_SETTING["HEATING_PID_OUTPUT_BIND"]
  GlobalData["FAN_PID_OUTPUT_BIND"] = TMP_STORED_SETTING["FAN_PID_OUTPUT_BIND"]
  return GlobalData, pid_time
end

